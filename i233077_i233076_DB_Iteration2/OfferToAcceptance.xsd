              <Mapping SourceColumn="CompanyName" DataSetColumn="CompanyName" />
              <Mapping SourceColumn="TotalOffers" DataSetColumn="TotalOffers" />
              <Mapping SourceColumn="TotalAcceptedApplications" DataSetColumn="TotalAcceptedApplications" />
              <Mapping SourceColumn="OfferToAcceptanceRatio" DataSetColumn="OfferToAcceptanceRatio" />
            </Mappings>
            <Sources />
          </TableAdapter>
        </Tables>
        <Sources />
