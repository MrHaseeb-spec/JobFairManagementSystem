                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT 
    AR.degreeProgram,
    COUNT(DISTINCT CASE WHEN HO.finalStatus = 'Hired' THEN S.studentId END) * 100.0 /
    NULLIF(COUNT(DISTINCT S.studentId), 0) AS placementRate
FROM STUDENT S
JOIN ACADEMIC_RECORD AR ON S.studentId = AR.studentId
LEFT JOIN APPLICATION A ON S.studentId = A.studentId
LEFT JOIN INTERVIEW I ON A.applicationId = I.applicationId
LEFT JOIN HIRING_OUTCOME HO ON I.interviewId = HO.interviewId
GROUP BY AR.degreeProgram;
</CommandText>
