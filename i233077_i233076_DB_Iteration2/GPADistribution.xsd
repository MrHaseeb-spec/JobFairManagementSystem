                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT 
  CASE 
    WHEN gpa &gt;= 0.0 AND gpa &lt; 1.0 THEN '0.0 - 0.9'
    WHEN gpa &gt;= 1.0 AND gpa &lt; 2.0 THEN '1.0 - 1.9'
    WHEN gpa &gt;= 2.0 AND gpa &lt; 3.0 THEN '2.0 - 2.9'
    WHEN gpa &gt;= 3.0 AND gpa &lt; 3.5 THEN '3.0 - 3.4'
    WHEN gpa &gt;= 3.5 AND gpa &lt;= 4.0 THEN '3.5 - 4.0'
  END AS GPABucket,
  COUNT(*) AS StudentCount
FROM ACADEMIC_RECORD
GROUP BY 
  CASE 
    WHEN gpa &gt;= 0.0 AND gpa &lt; 1.0 THEN '0.0 - 0.9'
    WHEN gpa &gt;= 1.0 AND gpa &lt; 2.0 THEN '1.0 - 1.9'
    WHEN gpa &gt;= 2.0 AND gpa &lt; 3.0 THEN '2.0 - 2.9'
    WHEN gpa &gt;= 3.0 AND gpa &lt; 3.5 THEN '3.0 - 3.4'
    WHEN gpa &gt;= 3.5 AND gpa &lt;= 4.0 THEN '3.5 - 4.0'
  END
ORDER BY GPABucket
</CommandText>
