                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT 
    CAST(COUNT(DISTINCT CASE WHEN HO.finalStatus = 'Hired' THEN A.studentId END) AS FLOAT) / 
    NULLIF(COUNT(DISTINCT A.studentId), 0) * 100 AS PlacementPercentage
FROM 
    APPLICATION A
JOIN 
    INTERVIEW I ON A.applicationId = I.applicationId
LEFT JOIN 
    HIRING_OUTCOME HO ON I.interviewId = HO.interviewId;</CommandText>
