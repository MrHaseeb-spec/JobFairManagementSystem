                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT 
    C.companyId,
    COUNT(I.interviewId) AS TotalInterviews
FROM 
    INTERVIEW I
JOIN 
    RECRUITER R ON I.recruiterId = R.recruiterId
JOIN 
    COMPANY C ON R.companyId = C.companyId
WHERE 
    I.interviewStatus = 'Completed'
GROUP BY 
    C.companyId
ORDER BY 
    TotalInterviews DESC;
</CommandText>
